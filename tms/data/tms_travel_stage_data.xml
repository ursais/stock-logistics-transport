<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record id="travel_stage_draft" model="tms.travel.stage">
        <field name="name">Draft</field>
        <field name="sequence">10</field>
        <field name="fold" eval="False" />
        <field name="state">draft</field>
    </record>
    <record id="travel_stage_scheduled" model="tms.travel.stage">
        <field name="name">Scheduled</field>
        <field name="sequence">20</field>
        <field name="fold" eval="False" />
        <field name="state">scheduled</field>
    </record>
    <record id="travel_stage_progress" model="tms.travel.stage">
        <field name="name">In Progress</field>
        <field name="sequence">30</field>
        <field name="fold" eval="False" />
        <field name="state">progress</field>
    </record>
    <record id="travel_stage_done" model="tms.travel.stage">
        <field name="name">Done</field>
        <field name="sequence">40</field>
        <field name="fold" eval="False" />
        <field name="state">done</field>
    </record>
    <record id="travel_stage_cancel" model="tms.travel.stage">
        <field name="name">Cancelled</field>
        <field name="sequence">50</field>
        <field name="fold" eval="True" />
        <field name="state">cancel</field>
    </record>
    <record id="travel_stage_closed" model="tms.travel.stage">
        <field name="name">Closed</field>
        <field name="sequence">60</field>
        <field name="fold" eval="True" />
        <field name="state">closed</field>
    </record>
</odoo>
